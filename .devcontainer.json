{
	"name": "Go for my projects",
	"image": "quay.io/deis/go-dev:v1.22.3",
	"extensions": [
		"ms-vscode.go"
	],
	"runArgs": [
		"--cap-add=SYS_PTRACE",
		"--security-opt",
		"seccomp=unconfined"
	],
	"postCreateCommand": "ln -sf /workspaces /go/src/github.com/ohthehugemanatee",
	"settings": {
		"go.gopath": "/go"
	}
}